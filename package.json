{
  "name": "karnue-website-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "Karnue premium ride-hailing service website",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "cd apps/landing && npm run dev",
    "build": "cd apps/landing && npm run build",
    "export": "cd apps/landing && npm run export",
    "start": "cd apps/landing && npm run start",
    "lint": "cd apps/landing && npm run lint"
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  }
}
